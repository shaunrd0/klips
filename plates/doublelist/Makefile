CXX=g++
CXXFLAGS=-g -Wall

###############################################################################
# Driver
###############################################################################

driver:	driver.cpp	doublelist.o
	${CXX} ${CXXFLAGS} driver.cpp doublelist.o -o driver

###############################################################################
# DoubleList
###############################################################################

doublelist.o:	doublelist.cpp doublelist.h
	${CXX} ${CXXFLAGS} -c doublelist.cpp -o doublelist.o

###############################################################################
# Clean
###############################################################################

clean:
	rm -f *.o driver
