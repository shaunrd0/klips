CXX=g++
CXXFLAGS=-g -Wall

###############################################################################
# Driver
###############################################################################

driver:	driver.cpp	stacklist.o
	${CXX} ${CXXFLAGS} driver.cpp stacklist.o -o driver

###############################################################################
# StackList
###############################################################################

stacklist.o:	stacklist.cpp stacklist.h
	${CXX} ${CXXFLAGS} -c stacklist.cpp -o stacklist.o

###############################################################################
# Clean
###############################################################################

clean:
	rm -f *.o driver
