CXX=g++
CXXFLAGS=-g -Wall

###############################################################################
# Driver
###############################################################################

driver:	driver.cpp	vector.o
	${CXX} ${CXXFLAGS} driver.cpp vector.o -o driver

###############################################################################
# Vector
###############################################################################

vector.o:	vector.cpp vector.h
	${CXX} ${CXXFLAGS} -c vector.cpp -o vector.o

###############################################################################
# Clean
###############################################################################

clean:
	rm -f *.o driver
