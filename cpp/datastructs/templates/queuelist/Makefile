CXX=g++
CXXFLAGS=-g -Wall

###############################################################################
# Driver
###############################################################################

driver:	driver.cpp	queuelist.o
	${CXX} ${CXXFLAGS} driver.cpp queuelist.o -o driver

###############################################################################
# QueueList
###############################################################################

queuelist.o:	queuelist.cpp queuelist.h
	${CXX} ${CXXFLAGS} -c queuelist.cpp -o queuelist.o

###############################################################################
# Clean
###############################################################################

clean:
	rm -f *.o driver
